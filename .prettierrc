{
	"useTabs": true,
	"tabWidth": 4,
	"svelteSortOrder": "scripts-options-markup-styles",
	"svelteIndentScriptAndStyle": true,
	"singleAttributePerLine": true,
	"semi": false,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	]
}
